; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Land Of Caldoria"
run/main_scene="res://scenes/start.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://signal_bus.gd"
WorldData="*res://scenes/world/world_data.gd"
ItemLoader="*res://items/loader.gd"
ListLoader="*res://item_lists/loader.gd"
MobLoader="*res://scenes/mob/mob_loader.gd"
Settings="*res://settings.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
QuestHandler="*res://scenes/systems/quests/quest_handler.gd"
SaveManager="*res://scenes/save_manager.gd"
EventAudio="*res://addons/event_audio/scenes/event_audio_autoload.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Land Of Caldoria"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/ScriptEditorQoL/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/event_audio/plugin.cfg", "res://addons/gaea/plugin.cfg", "res://addons/rubonnek.quest_manager/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "yellow",
"res://item_lists/": "teal",
"res://items/": "green",
"res://scenes/player/": "pink",
"res://scenes/systems/": "blue",
"res://scenes/ui/hud/": "pink",
"res://themes/": "gray",
"res://tilesets/": "gray"
}

[gui]

theme/custom="res://themes/default.tres"
theme/custom_font="res://assets/fonts/m5x7.ttf"

